<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="wiz_hospital_management_form_view" model="ir.ui.view">
            <field name="name">wiz.hospital.management</field>
            <field name="model">wiz.hospital.management</field>
            <field name="arch" type="xml">
                <form string="Form">
                <sheet>
                        <group col="4" colspan="1">
                            <field name="name" />
                            <button type="object" name="print_report" string="Print Report" />
                        </group>

                </sheet>
                </form>
            </field>
        </record>
        <record id="action_wiz_hospital_management_view" model="ir.actions.act_window">
            <field name="name">Inner Detail</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wiz.hospital.management</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>


</odoo>